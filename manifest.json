{
   "manifest_version": 2,
   "version": "0.1.10",
   "name": "ICS Chromium theme",
   "description": "Android ICS inspired theme for Chrome.",
   "theme": {
      "colors": {
         "frame": [ 0, 0, 0 ],
         "ntp_text": [ 200, 200, 200 ],
         "tab_text": [ 200, 200, 200 ],
         "tab_background_text": [ 200, 200, 200 ],
         "bookmark_text": [ 200, 200, 200],
         "button_background": [ 200, 200, 200],
         "toolbar": [ 0, 0, 0],
         "ntp_background": [ 40, 40, 40 ]
      },
      "images": {
         "theme_toolbar": "images/tab_hover.png",
         "theme_frame": "images/background-2.png",
         "theme_button_background": "images/background-2.png",
         "theme_tab_background": "images/background.png",
         "theme_ntp_background": "images/background-2.png",
         "theme_ntp_attribution": "images/background-2.png"
      },
      "tints": {
         "buttons": [ 1, 1, 1 ]
      },
      "properties": {
        "ntp_background_repeat": "repeat"
      },
      "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
      }
   }
